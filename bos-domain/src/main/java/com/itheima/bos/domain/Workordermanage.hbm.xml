<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.itheima.bos.domain.Workordermanage" table="qp_workordermanage" >
        <id name="id" type="string">
            <column name="id" length="32" />
            <generator class="assigned" />
        </id>
        <property name="arrivecity" type="string">
            <column name="arrivecity" length="20" />
        </property>
        <property name="product" type="string">
            <column name="product" length="20" />
        </property>
        <property name="num" type="java.lang.Integer">
            <column name="num" />
        </property>
        <property name="weight" type="java.lang.Double">
            <column name="weight" precision="22" scale="0" />
        </property>
        <property name="floadreqr" type="string">
            <column name="floadreqr" />
        </property>
        <property name="prodtimelimit" type="string">
            <column name="prodtimelimit" length="40" />
        </property>
        <property name="prodtype" type="string">
            <column name="prodtype" length="40" />
        </property>
        <property name="sendername" type="string">
            <column name="sendername" length="20" />
        </property>
        <property name="senderphone" type="string">
            <column name="senderphone" length="20" />
        </property>
        <property name="senderaddr" type="string">
            <column name="senderaddr" length="200" />
        </property>
        <property name="receivername" type="string">
            <column name="receivername" length="20" />
        </property>
        <property name="receiverphone" type="string">
            <column name="receiverphone" length="20" />
        </property>
        <property name="receiveraddr" type="string">
            <column name="receiveraddr" length="200" />
        </property>
        <property name="feeitemnum" type="java.lang.Integer">
            <column name="feeitemnum" />
        </property>
        <property name="actlweit" type="java.lang.Double">
            <column name="actlweit" precision="22" scale="0" />
        </property>
        <property name="vol" type="string">
            <column name="vol" length="20" />
        </property>
        <property name="managerCheck" type="string">
            <column name="managerCheck" length="1" />
        </property>
        <property name="updatetime" type="date">
            <column name="updatetime" length="0" />
        </property>
    </class>
</hibernate-mapping>
